<ContentDialog
    x:Class="BBIHardwareSupport.MDM.IntuneConfigManager.Views.WorkspaceOneLoginDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Workspace ONE Login"
    PrimaryButtonText="Login"
    CloseButtonText="Cancel"
    DefaultButton="Primary"
    Width="400">

    <StackPanel Spacing="12">
        <TextBox x:Name="UsernameTextBox" Header="Username" PlaceholderText="Enter your username" />
        <PasswordBox x:Name="PasswordBox" Header="Password" />
        <TextBox x:Name="ApiKeyTextBox" Header="API Key" PlaceholderText="Enter API Key" />
    </StackPanel>
</ContentDialog>
